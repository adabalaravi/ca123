<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.accenture.ams.db.bean.TechnicalPackageProfile" table="TECHNICAL_PACKAGE">
	<id
        name="packageId"
        type="long"
        column="PACKAGE_ID"
        length="38"
    >
		<generator class="assigned" />
	</id>
	
	<property
        name="packageName"
        type="string"
        column="PACKAGE_NAME"
        length="100"
    />
    <property
        name="packageType"
        type="string"
        column="PACKAGE_TYPE"
        length="50"
    />
    
</class>
</hibernate-mapping>
