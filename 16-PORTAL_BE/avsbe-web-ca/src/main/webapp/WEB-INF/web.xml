<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>AVS</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>InitBackEndServlet</servlet-name>
    <servlet-class>
			com.accenture.avs.be.framework.initframework.InitBackEndServlet</servlet-class>
    <init-param>
      <param-name>ENV_BE_PROPERTIES_NAME</param-name>
      <param-value>BE_PROPERTIES</param-value>
    </init-param>
    <init-param>
      <param-name>DEFAULT_PROPERTIES_PATH</param-name>
      <param-value>/properties</param-value>
    </init-param>
    <init-param>
      <param-name>MAPPING_PROPERTIES_FILE</param-name>
      <param-value>/mapping.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TENANT_PROPERTIES_FILE</param-name>
      <param-value>/tenant.properties</param-value>
    </init-param>
    <init-param>
      <param-name>REC_TENANT_PROPERTIES_FILE</param-name>
      <param-value>/rec-tenant.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TR_MAPPING_PROPERTIES_FILE</param-name>
      <param-value>300000</param-value>
    </init-param>
    <init-param>
      <param-name>MAPPING_API_PROPERTIES_FILE</param-name>
      <param-value>/apibeetv.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TR_MAPPING_API_PROPERTIES_FILE</param-name>
      <param-value>300000</param-value>
    </init-param>
    <init-param>
      <param-name>BEETVGENRE_PROPERTIES_FILE</param-name>
      <param-value>/beetvgenres.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TR_BEETVGENRE_PROPERTIES_FILE</param-name>
      <param-value>300000</param-value>
    </init-param>
    <init-param>
      <param-name>MAPPING_WS_PROPERTIES_FILE</param-name>
      <param-value>/webservices.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TR_WS_PROPERTIES_FILE</param-name>
      <param-value>300000</param-value>
    </init-param>
    <init-param>
      <param-name>MAPPING_LOG4J_XML_FILE</param-name>
      <param-value>/log4j.xml</param-value>
    </init-param>
    <init-param>
      <param-name>MAPPING_LOG4J_PROPERTIES_FILE</param-name>
      <param-value>/log4j.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TR_MAPPING_LOG4J_PROPERTIES_FILE</param-name>
      <param-value>60000</param-value>
    </init-param>
    <init-param>
      <param-name>MAPPING_RSA_PROPERTIES_FILE</param-name>
      <param-value>/aes.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TR_MAPPING_RSA_PROPERTIES_FILE</param-name>
      <param-value>300000</param-value>
    </init-param>
    <init-param>
      <param-name>CONSTANTS_PARAMETER_PROPERTIES_FILE</param-name>
      <param-value>/constantsparameter.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TR_CONSTANTS_PARAMETER_PROPERTIES_FILE</param-name>
      <param-value>300000</param-value>
    </init-param>
    <init-param>
      <param-name>ERROR_MESSAGE_PROPERTIES_FILE</param-name>
      <param-value>/errormsg.properties</param-value>
    </init-param>
    <init-param>
      <param-name>TR_ERROR_MESSAGE_PROPERTIES_FILE</param-name>
      <param-value>300000</param-value>
    </init-param>
    <init-param>
      <param-name>LAST_UPDATE</param-name>
      <param-value>LAST_UPDATE</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <description></description>
    <display-name>AVSController</display-name>
    <servlet-name>AVSController</servlet-name>
    <servlet-class>com.accenture.avs.be.framework.AVSController</servlet-class>
    <init-param>
      <param-name>USE_QSTRING_FOR_TENANT_TEST</param-name>
      <param-value>false</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description>
		</description>
    <display-name>CheckConnection</display-name>
    <servlet-name>CheckConnection</servlet-name>
    <servlet-class>com.accenture.avs.be.framework.technical.CheckConnection</servlet-class>
    <init-param>
      <param-name>env_config_variable</param-name>
      <param-value>BE_PROPERTIES</param-value>
    </init-param>
    <init-param>
      <param-name>database_config_file_name</param-name>
      <param-value>/CC_database.properties</param-value>
    </init-param>
    <init-param>
      <param-name>status_ok</param-name>
      <param-value>0</param-value>
    </init-param>
    <init-param>
      <param-name>status_ko</param-name>
      <param-value>1</param-value>
    </init-param>
    <init-param>
      <param-name>status_not_valid</param-name>
      <param-value>-1</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>CaptchaGenerator</servlet-name>
    <servlet-class>com.accenture.avs.be.captchaServlet.captchaGenerator.CaptchaGenerator</servlet-class>
    <init-param>
      <param-name>USE_QSTRING_FOR_TENANT_TEST</param-name>
      <param-value>false</param-value>
    </init-param>
    <init-param>
      <param-name>BE_PROPERTIES_ENV</param-name>
      <param-value>BE_PROPERTIES</param-value>
    </init-param>
    <init-param>
      <param-name>TENANT_PROPERTIES_FILE</param-name>
      <param-value>captcha_tenant.properties</param-value>
    </init-param>
    <init-param>
      <param-name>SERVLET_PROPERTIES_FILE</param-name>
      <param-value>captchaGenerator.properties</param-value>
    </init-param>
  </servlet>
  <filter>
    <filter-name>AVSFilter</filter-name>
    <filter-class>com.accenture.avs.be.framework.AVSFilter</filter-class>
    <init-param>
      <param-name>USE_QSTRING_FOR_TENANT_TEST</param-name>
      <param-value>false</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>AVSFilter</filter-name>
    <servlet-name>AVSController</servlet-name>
  </filter-mapping>
  <servlet>
    <servlet-name>ReloadDS</servlet-name>
    <servlet-class>com.accenture.avs.be.db.util.servlet.ReloadDS</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitBackEndServlet</servlet-name>
    <url-pattern>/InitBackEndServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReloadDS</servlet-name>
    <url-pattern>/ReloadDS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AVSController</servlet-name>
    <url-pattern>/besc</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckConnection</servlet-name>
    <url-pattern>/CheckConnection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CaptchaGenerator</servlet-name>
    <url-pattern>/besc/CaptchaGenerator</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.accenture.avs.be.component.SessionHandler</listener-class>
  </listener>
  <listener>
    <listener-class>com.accenture.avs.ca.be.listener.StopContentCAListener</listener-class>
  </listener>
  <session-config>
    <session-timeout>20</session-timeout>
  </session-config>
</web-app>